{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "py: current file",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "cwd": "${fileDirname}",
            "console": "integratedTerminal"
        },
        {
            "name": "py: logging test",
            "type": "debugpy",
            "request": "launch",
            "module": "streamlit",
            "args": [
                "run",
                "src/main.py",
                "--server.headless",
                "true",
                "--server.port",
                "5012",
            ],
            "cwd": "${workspaceFolder}/logging/",
            "envFile": "${workspaceFolder}/logging/env/.env.dev",
            "consoleName": "logging"
        },
    ]
}